{
  "name": "moritz-sauer-13/brevo-newsletter-registration",
  "description": "Brevo Newsletter Registration",
  "authors": [
    {
      "name": "Tietge GmbH",
      "email": "ms@tietge.com"
    }
  ],
  "require": {
    "silverstripe/framework": "^6",
    "silverstripe/cms": "^6",
    "dnadesign/silverstripe-elemental": "^6",
    "getbrevo/brevo-php": "^2.0",
    "silverstripe/spamprotection": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Brevo\\NewsletterRegistration\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/scss",
      "client/js"
    ],
    "project-files-installed": [
      ".htaccess",
      "app/.htaccess",
      "app/_config/mimevalidator.yml",
      "app/_config/mysite.yml",
      "app/src/Page.php",
      "app/src/PageController.php"
    ],
    "public-files-installed": [
      ".htaccess",
      "index.php",
      "web.config"
    ]
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/recipe-plugin": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
